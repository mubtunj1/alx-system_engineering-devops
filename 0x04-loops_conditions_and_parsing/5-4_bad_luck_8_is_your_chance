#!/usr/bin/env bash
# A script that loops through 1 to 10 using while

i=1

while [ $i -le 20 ]
do
    if [ $i -eq 4 ]
    then
        echo "bad luck from China"
    elif [ $i -eq 9 ]
    then
        echo "bad luck from Japan"
    else
        echo "bad luck from Italy"
    fi
    ((i++))
done
